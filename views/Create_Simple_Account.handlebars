<body>
    <header>
        <h1 class="site-title"> <a href="#">Park It!</a> </h1>
    </header>

    <nav class="headerNav">
        <a href="/">Home</a>
        <a href="/search">Seach for Parking</a>
        <a href="/report_bug">Bug Report</a>
    </nav>

    <main class="contents">
        <h2>Create a new account</h2>
        <div class="create-content">
            <form id="MyForm" action="Create_A" method="POST">
                <label for="username" class="login-label">Email</label>
                <input type="text" name="username" id="username" class="login-input">
                <label for="password" class="login-label">Password</label>
                <input type="password" name="password" id="password" class="login-input">
                <label for="password" class="login-label">Confirm Password</label>
                <input type="password" name="confirm_password" id="confirm_password" class="login-input">
                <input type="submit" value="Submit" id="submit-button">
            </form>
        </div>
    </main>
</body>
